@model Entities.Book

@{
    ViewBag.Title = "Книга";
    ViewBag.IsAdmin = (string) TempData.Peek("login") == "admin";
    if (ViewBag.IsAdmin)
    {
        Layout = "~/Views/Shared/_LayoutAdmin.cshtml";
    }
    
}
<div class="card">
    <div class="card-header">
        <h5>@Model.BookTitle</h5>
    </div>
    <div class="card-body">

    </div>
</div>


